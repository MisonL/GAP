{
  "name": "gap-frontend",
  "version": "1.0.0",
  "description": "GAP (Gemini API Proxy) Frontend - Modern Vue.js web application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit -- --passWithNoTests",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "build-only": "vite build",
    "preview-only": "vite preview",
    "dev-host": "vite --host 0.0.0.0",
    "analyze": "vite build --mode analyze",
    "serve": "vite preview --host 0.0.0.0 --port 4173"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "pinia": "^2.3.0",
    "axios": "^1.7.9",
    "@vueuse/core": "^12.0.0",
    "@vueuse/integrations": "^12.0.0",
    "element-plus": "^2.9.1",
    "@element-plus/icons-vue": "^2.3.1",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "js-cookie": "^3.0.5",
    "crypto-js": "^4.2.0",
    "qrcode": "^1.5.4",
    "file-saver": "^2.0.5",
    "xlsx": "^0.18.5",
    "echarts": "^5.5.1",
    "vue-echarts": "^7.0.3",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "tailwindcss": "^3.4.16",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-i18n": "^10.0.5",
    "@vueuse/motion": "^2.2.6",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue-virtual-scroll-list": "^2.3.5",
    "chart.js": "^4.4.7",
    "prismjs": "^1.29.0",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "monaco-editor": "^0.52.2",
    "vue-monaco-editor": "^1.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "vite": "^6.0.3",
    "@vue/tsconfig": "^0.7.0",
    "vue-tsc": "^2.1.10",
    "typescript": "~5.6.3",
    "@types/node": "^22.10.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/crypto-js": "^4.2.2",
    "@types/nprogress": "^0.2.3",
    "@types/file-saver": "^2.0.7",
    "@types/qrcode": "^1.5.5",
    "@types/prismjs": "^1.26.5",
    "@playwright/test": "^1.49.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "vitest": "^2.1.8",
    "jsdom": "^25.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.16.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "prettier": "^3.4.2",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "tailwindcss": "^3.4.16",
    "sass": "^1.83.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.28.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^5.3.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "cross-env": "^7.0.3",
    "concurrently": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "commitizen": "^4.1.0",
    "standard-version": "^9.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.2",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less,styl}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript",
    "fastapi",
    "gemini",
    "api",
    "proxy",
    "ai",
    "chatbot"
  ],
  "author": "MisonL <misonl@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisonL/GAP.git"
  },
  "bugs": {
    "url": "https://github.com/MisonL/GAP/issues"
  },
  "homepage": "https://github.com/MisonL/GAP#readme"
}